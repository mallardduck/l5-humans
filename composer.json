{
    "name": "olssonm/l5-humans",
    "description": "Simple humans.txt routes and views for Laravel 5.",
    "keywords": [
        "olssonm",
        "Laravel 5",
        "humans.txt"
    ],
    "homepage": "https://github.com/olssonm/l5-humans",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "illuminate/support": "~5.4",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.7.*",
        "orchestra/testbench": "~3.4",
        "orchestra/testbench-browser-kit": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\Humans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\Humans\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
